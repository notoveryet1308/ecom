{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "quotes": ["error", "single"],
    "no-unused-vars": [
      "off",
      { "argsIgnorePattern": "req|res|next|val|process" }
    ],
    "object-shorthand": "off",
    "radix":"off"
  }
}
