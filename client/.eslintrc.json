{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "semi": "off",
        "no-console":"warn",
        "indent": "off",
        "no-tabs":"off",
        "linebreak-style":"off",
        "class-methods-use-this":"off",
        "implicit-arrow-linebreak":"off",
        "function-paren-newline":"off",
        "no-param-reassign":"warn",
        "object-curly-newline":"off",
        "radix":"off"
        
    }
}
